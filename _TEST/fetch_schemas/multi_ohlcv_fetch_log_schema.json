{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "type": "object",
  "properties": {
    "timestamp": {
      "type": "string",
      "format": "date-time"
    },
    "source_exchange": {
      "type": "string"
    },
    "symbol": {
      "type": "string"
    },
    "intervals": {
      "type": "array",
      "items": {
        "type": "string"
      }
    },
    "data_preview": {
      "type": "object",
      "patternProperties": {
        ".*": {
          "type": "object",
          "properties": {
            "rsi": { "type": "number" },
            "ema": { "type": "number" },
            "macd": { "type": "number" },
            "macd_signal": { "type": "number" },
            "bb_upper": { "type": "number" },
            "bb_lower": { "type": "number" },
            "close": { "type": "number" }
          },
          "required": ["rsi", "ema", "macd", "macd_signal", "bb_upper", "bb_lower", "close"],
          "additionalProperties": false
        }
      }
    },
    "limit": {
      "type": "integer"
    },
    "start_time": {
      "type": ["string", "null"],
      "format": "date-time"
    },
    "end_time": {
      "type": ["string", "null"],
      "format": "date-time"
    }
  },
  "required": ["timestamp", "source_exchange", "symbol", "intervals", "data_preview", "limit", "start_time", "end_time"],
  "additionalProperties": false
}
